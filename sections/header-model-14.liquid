  {% endif %}    
  
   {% if section.settings.display_minicart %}
                <li class="header-bar__module cart header_cart">
                  {% include 'mini-cart' %} 
                </li> 
                {% endif %}  
  
  
  {% if section.settings.display_wishlist %}   
        <li>            
          <a class="wishlist" href="/pages/wishlist" title="{{ 'products.wishlist.wishlist_name' | t }}"><svg class="icon" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:cc="http://creativecommons.org/ns#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:svg="http://www.w3.org/2000/svg" xmlns="http://www.w3.org/2000/svg" xmlns:sodipodi="http://sodipodi.sourceforge.net/DTD/sodipodi-0.dtd" xmlns:inkscape="http://www.inkscape.org/namespaces/inkscape" version="1.1" x="0px" y="0px" viewBox="0 0 100 125"><g transform="translate(0,-952.36218)"><path d="m 33.780493,970.3622 c -11.63849,0 -18.78049,10.17801 -18.78049,18.52632 0,18.05128 20.51725,34.80698 35,45.47368 14.482764,-10.6667 34.999994,-27.4224 34.999994,-45.47368 0,-8.34831 -7.14198,-18.52632 -18.78046,-18.52632 -6.50001,0 -12.052864,5.1822 -16.219534,10.10527 -4.16666,-4.92307 -9.71959,-10.10527 -16.21951,-10.10527 z" style="color:#000000;enable-background:accumulate;"  stroke="none" marker="none" visibility="visible" display="inline" overflow="visible"/></g></svg></a>           
        </li>
        {% endif %} 
  <li class="header-mobile">
    <div class="menu-block visible-phone"><!-- start Navigation Mobile  -->
      <div id="showLeftPush">
        <i class="fa fa-bars" aria-hidden="true">  </i>
      </div>
    </div><!-- end Navigation Mobile  --> 
  </li>
 
</ul>


<style> 
  /* Top block */
  .header-type-14 .top_bar { background: {{ section.settings.top_bar_bg }}; }
  .header-type-14 .top_bar li, .header-type-14 .top_bar span { color:{{section.settings.top_bar_text}};}   
  .header-type-14 .top_bar a { color:{{section.settings.top_bar_links }};}    
  .header-type-14 .top_bar a:hover, .header-type-14 .top_bar a:hover span { color:{{section.settings.top_bar_link_hover }};}    

  /* Logo block */
  .header-type-14 .site-header__logo a { color:{{section.settings.logo_text }};}
  .header-type-14 .site-header__logo a:hover { color:{{section.settings.logo_hover_text }};}    

  /* Menu  block */
  .header-type-14 .site-header,.mobile-nav-section ,.header-type-14 .sticky-animate{background: {{ section.settings.menubar_bg }};}
  .header-type-14 .menu-tool ul li {color: {{ section.settings.menubar_text }};}
  .header-type-14 .menu-tool ul li a,.mobile-nav-section .mobile-nav-trigger {color:{{section.settings.menubar_links}};}  
  .header-type-14 .menu-tool ul li a:hover,.header-type-14 .menu-tool .site-nav > li > a.current:hover {color:{{section.settings.menubar_links_hover}};} 
  .header-type-14 .menu-tool .site-nav >  li > a.current {color:{{section.settings.menu_active}};} 
  .header-type-14 .site-nav-dropdown,#MobileNav,.mobile-nav__sublist { background: {{ section.settings.menubar_dropdown_bg }};}
  .header-type-14 .site-nav-dropdown .inner > a {color: {{ section.settings.menubar_dropdown_caption }};}    
  .header-type-14 .site-nav-dropdown .inner > a:hover {color: {{ section.settings.menubar_dropdown_caption_hover }};}    
  .header-type-14 .site-nav-dropdown .inner .dropdown a,.header-type-14 .menu-tool .site-nav .site-nav-dropdown li a,.header-type-14 .site-nav .widget-featured-product .product-title,.header-type-14 .site-nav .widget-featured-product .widget-title h3,#MobileNav a,.mobile-nav__sublist a,.site-nav .widget-featured-nav .owl-prev a,.site-nav .widget-featured-nav .owl-next a  {color: {{ section.settings.link_color }};}

  /* Dropdown block */
  .header-type-14 .menubar-section #Togglemodal i {color: {{ section.settings.dropdown_icon }};}
  .header-type-14 .menubar-section #Togglemodal i:hover {color: {{ section.settings.dropdown_icon_hover }};}
  .header-type-14 #slidedown-modal {background: {{ section.settings.dropdown_bg }};}
  .header-type-14 #slidedown-modal ul li a {color:{{section.settings.dropdown_link}};} 
  .header-type-14 #slidedown-modal ul li a:hover {color:{{section.settings.dropdown_hover_link}};} 

  
  /* Cart Summary block */
    .header-type-14 a.icon-cart-arrow i  {color: {{ section.settings.minicart_icon }};}
    .header-type-14 a.icon-cart-arrow:hover i {color: {{ section.settings.minicart_icon_hover }};}
    .header-type-14 #slidedown-cart .actions, .header-type-14  #slidedown-cart  {background: {{ section.settings.minicart_bg }};}
    .header-type-14 .header-bar__module p {color: {{ section.settings.minicart_text }};}
    .header-type-14 .header-bar__module a {color:{{section.settings.minicart_link }};}  
    .header-type-14 .header-bar__module a:hover {color:{{section.settings.minicart_link_hover}};} 
    .header-type-14 .header-bar__module .btn {color:{{ settings.minicart_btn_text}};background: {{ section.settings.minicart_btn_bg }};border:2px solid {{ section.settings.minicart_btn_hover_bg }};} 
    .header-type-14 .header-bar__module .btn:hover {color:{{section.settings.minicart_btn_hover_text}};background: {{ section.settings.minicart_btn_hover_bg }};} 
    .header-type-14  #slidedown-cart .total .price, .header-type-14 #minicart_total,.header-type-14 #slidedown-cart ul li .cart-collateral {color:{{section.settings.minicart_total }};} 

  

  /* Search block */     
  .header-type-14 .search-bar input[type="search"],.header-type-14 .sb-search.sb-search-open .sb-icon-search,.header-type-14 .results-box a {color:{{section.settings.search_text }};} 
  .header-type-14 .header-search span  {color:{{section.settings.search_icon}};} 
  .header-type-14 .header-search span:hover,.header-type-14 .sb-search .sb-icon-search:hover {color:{{section.settings.search_icon_hover }};} 
  .header-type-14 .search-bar__form, .header-type-14 #SearchDrawer,.drawer-page-content:after,.header-type-14 .sb-search.sb-search-open .sb-icon-search{  background:{{section.settings.search_bg }};}
  .header-type-14 .results-box {background:{{ section.settings.minicart_count}}; }
  .header-type-14 .search-bar__form button,.header-type-14 .search-bar__icon-button { color:{{section.settings.search_link }};}
  .header-type-14 .search-bar__form button:hover,.header-type-14 .search-bar__icon-button:hover { color:{{section.settings.search_link_hover }};}
  .header-type-14 .header-panel-top ul li.currency-block,.header-type-14 #slidedown-cart li { border-bottom:1px solid {{ section.settings.border_between_bars}}; }

  .header-type-14 .search-bar input[type="search"]::-webkit-input-placeholder  { /* Chrome/Opera/Safari */
    color:{{section.settings.search_text }};
  }
  .header-type-14 .search-bar input[type="search"]::-moz-placeholder { /* Firefox 19+ */
    color:{{section.settings.search_text }};
  }
  .header-type-14 .search-bar input[type="search"]:-ms-input-placeholder { /* IE 10+ */
    color:{{section.settings.search_text }};
  }
  .header-type-14 .search-bar input[type="search"]:-moz-placeholder { /* Firefox 18- */
    color:{{section.settings.search_text }};
  }
  .header-type-14 .results-box img{border:1px solid {{ section.settings.border_between_bars}}; }
  .header-type-14 .menu-icon li a , .header-type-14 .header-mobile {color:{{section.settings.logo_text}};}
  .header-type-14 .menu-icon li a:hover,.header-type-14 .header-mobile:hover{color:{{section.settings.logo_hover_text}};}
  /* Header borders */


  .header-type-14 .menu-icon > li{border-left:1px solid {{section.settings.border_between_bars}};}
  .header-type-14 .customer_account ul li:nth-child(2){ border-left:1px solid {{section.settings.border_between_bars}};}

 
 .header-type-14 .header-bar__module.cart .baskettop a.icon-cart-arrow #cartCount { background: {{ section.settings.minicart_count_bg }};color:{{section.settings.minicart_count }};}
 .header-type-14 .header-bar__module.cart .baskettop a.icon-cart-arrow:hover #cartCount { background: {{ section.settings.minicart_count_hover_bg }};color:{{section.settings.minicart_count_hover_text }};}
 .header-type-14 .header-bar__module.cart .baskettop a.icon-cart-arrow #cartCount { background: {{ section.settings.minicart_count_bg }};color:{{section.settings.minicart_count }};}
 .header-type-14 .header-bar__module.cart .baskettop a.icon-cart-arrow:hover #cartCount { background: {{ section.settings.minicart_count_hover_bg }};color:{{section.settings.minicart_count_hover_text }};}
  .header-type-14 .menu-icon li a svg.icon{ fill:{{ section.settings.search_icon}};}
  .header-type-14 .menu-icon li a svg.icon:hover{ fill:{{ section.settings.search_icon_hover}};}
  .header-type-14 .sb-search input[type="search"]::placeholder,.header-type-14 .sb-search input[type="search"]{ color:{{ section.settings.search_icon}};}
  

</style> 


