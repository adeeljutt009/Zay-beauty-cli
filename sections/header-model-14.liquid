
<ul class="menu-icon">   
  
  {% if section.settings.display_search %}                
  <li class="header-search post-large--hide large--hide medium-down--hide">
    <div class="header-search ajaxsearch">
      <div id="sb-search" class="sb-search sb-search sb-search-open search-box large--hide" >
        <form action="/search" method="get" class="search-bar__table-cell search-bar__form" role="search">							
          {% if settings.show_search_prt %} 
          <input type="hidden" name="type" value="product">  
          {% endif %}
          <input type="search" id="search" name="q" value="{{ search.terms | escape }}" placeholder="{{ 'general.search.placeholder' | t }}" aria-label="{{ 'general.search.placeholder' | t }}" class="search-bar__input sb-search-input">
          <input class="sb-search-submit" type="submit" value="">
          <span class="sb-icon-search fa fa-search"></span>
        </form>
      </div>
    </div>

  </li>
  {% endif %} 
  
  
  
  
  
  
  {% if shop.customer_accounts_enabled or section.settings.display_wishlist %}
  <li class="menu_bar_right">
    <div class="customer_account">                          
      <ul>
        {% if shop.customer_accounts_enabled %}
        {% if customer %}
        <li >
          <a href="/account" title="{{ 'layout.customer.account' | t }}" data-value="value 1"> <svg class="icon" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" x="0px" y="0px" viewBox="0 0 100 125" style="enable-background:new 0 0 100 100;" xml:space="preserve"><g><path d="M87.4,85.5c0-20.8-13.3-32.3-37.4-32.3S12.6,64.7,12.6,85.5v4h74.9V85.5z"/><path d="M50,9.3c-11.4,0-20,8.8-20,20.4c0,11.9,9,21.6,20,21.6s20-9.7,20-21.6C70,18.1,61.4,9.3,50,9.3z"/></g></svg></a>
        </li>
        <li class="logout-link">
          <a title="{{ 'layout.customer.log_out' }}" href="/account/logout" ><svg class="icon" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" x="0px" y="0px" viewBox="0 0 100 125" style="enable-background:new 0 0 100 100;" xml:space="preserve"><g><g><path d="M66.2,67.6l5,5c0.5,0.5,1.1,0.5,1.6,0l21.9-21.9c0.5-0.5,0.5-1.1,0-1.5l-21.9-22c-0.5-0.5-1.1-0.5-1.6,0l-5,5    c-0.5,0.5-0.5,1.1,0,1.6l11.4,11.4H33.3c-0.7,0-1.1,0.5-1.1,1.1v7.1c0,0.7,0.5,1.1,1.1,1.1h44.4L66.2,66.1    C65.8,66.5,65.8,67.3,66.2,67.6z M14.4,92.2h37.5c5.2,0,9.4-4.2,9.4-9.4V65.3c0-0.7-0.6-1.2-1.2-1.2h-7c-0.7,0-1.2,0.6-1.2,1.2V79    c0,2.2-1.7,3.8-3.8,3.8H18.1c-2.2,0-3.8-1.7-3.8-3.8V21c0-2.2,1.7-3.8,3.8-3.8h29.9c2.2,0,3.8,1.7,3.8,3.8v13.7    c0,0.7,0.6,1.2,1.2,1.2h6.9c0.7,0,1.2-0.6,1.2-1.2V17.2c0-5.2-4.2-9.4-9.4-9.4H14.4C9.2,7.8,5,12,5,17.2v65.7    C5,88,9.2,92.2,14.4,92.2z"/></g></g></svg></a>
        </li>
        {% else %}
        <li >
          <a href="/account/login" title="{{ 'layout.customer.log_in' | t }}"> <svg class="icon" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" x="0px" y="0px" viewBox="0 0 100 125" style="enable-background:new 0 0 100 100;" xml:space="preserve"><g><path d="M87.4,85.5c0-20.8-13.3-32.3-37.4-32.3S12.6,64.7,12.6,85.5v4h74.9V85.5z"/><path d="M50,9.3c-11.4,0-20,8.8-20,20.4c0,11.9,9,21.6,20,21.6s20-9.7,20-21.6C70,18.1,61.4,9.3,50,9.3z"/></g></svg></a>
